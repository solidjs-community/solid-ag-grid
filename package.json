{
  "name": "snowpilot-solid-ag-grid",
  "version": "0.0.200",
  "description": "AG Grid SolidJS Component",
  "license": "MIT",
  "author": "Ben Warren <ben@snowpilot.com>",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/warrenbhw/solid-ag-grid"
  },
  "homepage": "https://github.com/warrenbhw/solid-ag-grid",
  "bugs": {
    "url": "https://github.com/warrenbhw/solid-ag-grid/issues"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/index.jsx",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "scripts": {
    "dev": "bun run --watch src/index.tsx",
    "clean": "rm -rf dist",
    "build": "bun run clean && bun build src/index.tsx --outdir=dist --target=node",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build-prod": "bun run build && bun run build:types",
    "format": "prettier -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "update-deps": "bun update --latest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@ag-grid-community/core": "31.1.1",
    "solid-js": ">=1.0.0"
  },
  "devDependencies": {
    "prettier": "3.2.5",
    "solid-js": "^1.8.16",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "vite-plugin-solid": "^2.10.2"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "bun@1.1.22"
}